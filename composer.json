{
    "name": "ilegion/sitemap",
    "description": "A simple package for generate sitemap xml.",
    "license": "MIT",
    "keywords": [
        "ilegionxs",
        "sitemap",
        "laravel-sitemap"
    ],
    "version": "0.0.2-dev",
    "autoload": {
        "psr-4": {
            "Ilegionxs\\Sitemap\\": "src/"
        },
        "classmap": ["src/"]
    },
    "authors": [
        {
            "role": "Developer",
            "name": "Yusif Zourob",
            "email": "wordlearningpress@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^8.1",
        "ext-zlib": "*",
        "illuminate/support": "^10.43"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.5"
    },
    "extra": {
        "laravel": {
            "providers": [
                "ilegionxs\\Sitemap\\SitemapServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true,
        "optimize-autoloader": true
    }
}
